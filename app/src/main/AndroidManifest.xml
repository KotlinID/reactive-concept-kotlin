<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools"
          package="com.baculsoft.sample.kotlinreactive">

    <application
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="AllowBackup,GoogleAppIndexingWarning">

        <activity android:name="com.baculsoft.sample.kotlinreactive.main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.baculsoft.sample.kotlinreactive.observable.ObservableActivity" />
        <activity android:name="com.baculsoft.sample.kotlinreactive.flowable.FlowableActivity" />
        <activity android:name="com.baculsoft.sample.kotlinreactive.single.SingleActivity" />
        <activity android:name="com.baculsoft.sample.kotlinreactive.completable.CompletableActivity" />
        <activity android:name="com.baculsoft.sample.kotlinreactive.maybe.MaybeActivity" />
    </application>
</manifest>